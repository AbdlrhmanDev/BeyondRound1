  "scripts": {
    "dev": "next dev --turbo",
    "dev:no-turbo": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:3001": "next dev -p 3001",
    "clean": "node scripts/clean-next.js",
    "fresh": "node -e \"const fs=require('fs');try{fs.rmSync('.next',{recursive:1,force:1});console.log('Cleaned .next')}catch(e){};try{fs.rmSync('node_modules/.cache',{recursive:1,force:1});console.log('Cleaned cache')}catch(e){};console.log('Done. Run: npm run dev')\"",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "preanalyze": "node -e \"try{require('fs').rmSync('.next',{recursive:true,force:true});console.log('Cleaned .next')}catch(e){}\"",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "optimize:hero": "node scripts/optimize-hero.mjs",
    "pwa:icons": "node scripts/generate-pwa-icons.mjs"
  },
