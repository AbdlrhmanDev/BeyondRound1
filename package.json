{
  "name": "beyondrounds",
  "version": "1.0.0",
  "private": true,
  "browserslist": [
    "Chrome >= 111",
    "Firefox >= 111",
    "Safari >= 16.4",
    "Edge >= 111",
    "not dead",
    "not op_mini all"
